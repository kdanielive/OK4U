# platform :ios, '9.0'

target 'OK4U' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OK4U
  pod 'Firebase/Analytics'
  pod 'Firebase/Auth'
  pod 'GoogleSignIn'
  pod 'Firebase/Database'
  pod 'SideMenu'
  pod 'Firebase/Firestore'
  pod 'Firebase/Core'
  pod 'Firebase/Storage'

  target 'OK4UTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'OK4UUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
